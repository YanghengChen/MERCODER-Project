<!DOCTYPE html>
<html>
<% var rootDir = process.cwd() %>

<head>
  <title>Coding Questions</title>
  <link href="/css/list.css" rel="stylesheet" type="text/css"/>
  <%- include(rootDir + '/views/partials/head'); %>
</head>

<body style="padding-top: 80px;">
  <!-- Navbar -->
  <%- include(rootDir + '/views/partials/navbar'); %>

  <div class="question-list">
    <h1>Coding Questions</h1>
    <p>Here are some common coding questions:</p>
    <ul id="question-list">
      <% for(var i = 0; i < questions.length; i++) { %>
        <li>
          <h3><%= questions[i].title %></h3>
          <p class="question-desc"><%= questions[i].description %></p>
          <button>View</button>
        </li>
      <% } %>
    </ul>
  </div>
</body>

</html>